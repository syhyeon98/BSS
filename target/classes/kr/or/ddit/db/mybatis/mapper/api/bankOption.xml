<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.ddit.finapi.dao.BankOptionDAO">

	
	<select id="selectBankOptionList" resultType="kr.or.ddit.finapi.vo.BankOptionVO">
		SELECT DISTINCT AREA_CD,AREA_NM FROM BANK_OPTION
		ORDER BY AREA_CD
	</select>

</mapper>